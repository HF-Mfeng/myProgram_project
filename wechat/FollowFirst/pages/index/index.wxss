#pyg_index{
  height: 100%;
}

page{
  height: 100%;
  background-color: var(--themColor);
  --mySize:750;
}

/* 轮播图的样式开始 */
#swiper_view{
  width: var(--mySize);
  height: 340rpx;
}
#swiper_swiper{
  width: 750rpx;
  height: 340rpx;
}
#swiper_image{
  width: 100%;
}
/* ----------轮播图的样式结束-------- */


/* 导航图的样式开始 */
#cate_view{
  display: flex;
  width: 750rpx;
  height: 200rpx;
}
#cate_navigator{
  flex: 1;
  padding: 30rpx;
  width: 750rpx;
  height: 220rpx;
}
#cate_image{
  width: 100%;
}
/* ----------导航图的样式结束-------- */

/* 楼层图的样式开始 */
.floor_view{
  width: 750rpx;
  height: 700rpx;
  /* height还需要调参 */
}
.floor_title_view{
  width: 750rpx;
  height: 60rpx;
  /* float: left; */
}
.floor_content_view{
  display: flex;
  width: 100%;
  height: 100%;
}
.floor_content_phote1{
  width: 33.33%;
  height: 100%;
}
.floor_content_photes2{
  width: 50%;
  height: 50%;
}
.photos_view{
  width: 66.66%;
  height: 100%;
}

/* ----------楼层图的样式结束-------- */
